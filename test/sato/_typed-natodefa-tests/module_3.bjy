let sample_module = 
    let t = List 'a in
    { type = { t = t },
    module = 
    { add = fun x -> fun s -> x :: s
    }}
in
let module_type = fun t -> {: add : ('a -> t -> t) :} in
let inner_t = (sample_module.type).t in
let module_type_check = module_type inner_t in
let (check_type : module_type_check) = sample_module.module in sample_module

