
let t = {: t : type ; elt : type ; empty : t ; add : t -> elt -> t :}

letd F (x : t) : { of_list : (list x.elt) -> x.t } =
  { of_list = fun ls -> 
    let rec loop acc ls =
      match ls with
      | [] -> acc
      | hd :: tl -> loop (x.add acc hd) tl
      end
    in
    loop x.empty ls
  }

let (M : t) = { elt = int ; t = list int ; empty = [] ; add = fun ls a -> a :: ls }

let X = F M 
