let Arg = sig
  val A : type
  val Honorificabilitudinitatibus : type
  val X : Honorificabilitudinitatibus
  val Y : A
end

let F (A : Arg) : sig end = struct
  (* open A *) (* We can't open in Bluejay, so we have to redefine*)
  let Honorificabilitudinitatibus = A.Honorificabilitudinitatibus
  let X = A.X
  let Y = A.Y
  let A = A.A
  
  let G(X:A)(Y:A)(_:A)(Z:A) : sig end = struct end
  let u = (G(X)(Y)(X)(Y)(X)).t (* ERROR: there is an extra argument, and the arguments have the wrong types *)
end