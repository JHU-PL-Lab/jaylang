let s = (X : sig val when_ : type val shall : type val we : type val three : type val meet : type val again :type end)
  -> (Y : sig val in_ : type val thunder : in_ val lightning : in_ val rain : type end)
  -> (Z : sig val when_ : type val the : type val hurlyburlys : type val done_ : type  end)
  -> (Z : sig val when_ : type val the : type val battles : type val lost : type val and_ : type val won : type end)
  -> (W : sig val that : type val will : type val be : type val ere : type val the_ : type val set : type val of_ : type val sun : type end)
  -> (S : sig val where : type val the : type val place : type end)
  -> (R : sig val upon : type val the : type val heath : type end)
  -> sig end

let F : s =
  let F
    (X : sig val when_ : type val shall : type val we : type val tree : type val meet : type val again : type end) (* ERROR: `tree` instead of `three` *)
    (Y : sig val in_ : type val thunder : in_ val lightning : in_ val pain : type end)
    (Z : sig val when_ : type val the : type val hurlyburlys : type val gone : type end) (* ERROR: `gone` instead of `done_` *)
    (Z : sig val when_ : type val the : type val battles : type val last : type val and_ : type val won : type end)
    (W : sig val that : type val will : type val be : type val the : type val era : type val set : type val of_ : type val sun : type end) (* ERROR: the middle few are mixed up *)
    (S : sig val where : type val the : type val lace : type end) (* ERROR: `lace` instead of `place` *)
    (R : sig val upon : type val the : type val heart : type end) (* ERROR: heart instead of `heath` *)
    : sig end
    = struct end
  in
  F
