let F
    (dependent A : sig val x : type val y : type end)
    (dependent B : sig val x = A.x end)
    (dependent C : sig val y = A.y end)
  : sig end
  = struct end

let M = struct
  let N = struct
    let x = abstract
    let y = abstract
  end
end

let Defs = struct
  let X = struct let x = M.N.x end
  let Y = struct let y = M.N.y end
end

(* We leave this as a partial application because the "too_many_xs" test does the full application *)
let Missing_X = F(M.N)(Defs.X)