# error is 0 `n` arg to `main` means `g` tries to unclock and already-unlocked state

let eq i =
  fun x ->
    i == x

let one_of i =
  { int | eq i }

let lock (st : one_of 0) : one_of 1 = 1

let unlock (st : one_of 1) : one_of 0 = 0

let f n st =
  if n > 0
  then lock st
  else st

let g n st =
  if n > 0
  then unlock st
  else st

let main (n : int) : one_of 0 =
  g n (f n 0)
