# error is that input list xs should be nonempty

let is_nonempty ls =
  match ls with
  | [] -> false
  | _ -> true
  end

let rec foldl1 (type a_) (f : a_ -> a_ -> a_) (xs : { List a_ | is_nonempty } ) : a_ =
  match xs with
  | z :: zs ->
    match zs with 
    | [] -> z
    | z1 :: tl -> foldl1 a_ f ((f z z1) :: tl)
    end
  end
