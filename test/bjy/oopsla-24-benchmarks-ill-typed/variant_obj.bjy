
let (obj : (((``A unit) -> int) && ((``B unit) -> int) && ((``C int) -> list int))) =
  let ob =
    fun self ->
      fun arg ->
        match arg with
        | `A u -> self self (`B {}) + 10
        | `B u -> 0
        | `C i -> self self (`A {}) :: self (`B {}) :: [ i ] # ERROR: forgot to self-apply on `B call
        end
  in
  ob ob
