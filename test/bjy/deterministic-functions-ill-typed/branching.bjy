(*
  This easy to solve because we use intensional equality.
*)
let _ : (int --> int) -> int -> int =
  fun f x ->
    if f x <> 11 then
      1
    else if f 0 <> 121 then
      7
    else if f (x + 10) <> 121 then
      2
    else if x <> (0 - 10) then
      9
    else 
      not x
