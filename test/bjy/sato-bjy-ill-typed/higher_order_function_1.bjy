let isEven x = (x % 2) == 0

let isOdd x = (x % 2) <> 0

let e2o (f : { int | isEven} -> { int | isEven}) : { int | isOdd } -> { int | isOdd } =
  fun n -> (f (n + 1)) (* ERROR: need to add one to the result to make sure it is even, or return type should be even *)
    