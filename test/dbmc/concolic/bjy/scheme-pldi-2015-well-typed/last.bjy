# type error is that list can be empty and has no last. An `is_nonempty` predicate fixes it.

# y combinator
let rec ycomb f =
  fun y ->
    (fun x -> f (fun z -> (x x) z))
    (fun x -> f (fun z -> (x x) z))
    y
in

let last (l : List 'a) : 'a =
  ycomb (
    fun self x ->
      match x with
      | [] -> assert false
      | x :: xs -> 
        match xs with
        | [] -> x
        | _ -> self xs
        end
      end
  ) l
in

last