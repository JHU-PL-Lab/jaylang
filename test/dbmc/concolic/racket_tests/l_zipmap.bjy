# main 1 returns 1 so fails contract

let rec zip x y =
  if x == 0 and y == 0
  then x
  else
    if x <> 0 and y <> 0
    then 1 + zip (x - 1) (y - 1)
    else assert false
in

let rec map x =
  if x == 0
  then x
  else 1 + map (x - 1)
in

letd main (n : int) : {. int | fun i -> i <> n } =
  map (zip n n)
in
main