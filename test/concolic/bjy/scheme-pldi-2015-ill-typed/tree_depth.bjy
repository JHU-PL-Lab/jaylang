
let t = Mu tt.
  if input == 123456789
  then ``Node {: left : tt , right : tt :}
  else ``Leaf {: leaf : bool :}
in

let max x y =
  if x > y
  then x
  else y
in

let rec depth (tree : t) : {. int | fun i -> i > 0 } =
  match tree with
  | `Leaf leaf -> 0 # ERROR: should be `1` or change predicate above s.t. returned int is non-negative
  | `Node node -> 1 + max (depth node.left) (depth node.right)
  end
in

depth
