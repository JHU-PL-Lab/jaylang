# EXPECT-WELL-FORMED
# EXPECT-INPUT-SEQUENCES-REACH work_end [0]
# The input of [1] [2] [3] takes more than 10000 steps

## 
# int work(int w) { 
#    int i,j=0;
#    for(i=0;i<w;i++) j++;
#    return 0; 
# }
##

let loop_end = 1
in let work w =
  let rec work_loop j i =
    if i == w
    then let work_end = loop_end in work_end
    else (
      work_loop (j+1) (i+1)
    )
  in let ignored = work_loop 0 0
  in 0
in let x = input
in work x